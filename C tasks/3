#include <stdio.h>
int main(void)
{
float a;
float b;
float c;
float d;
float r;
float p;
   printf("Enter the complex resistance: a+bi, c+di \n ");
   scanf("%f+%fi, %f+%fi", &a, &b, &c, &d);
r=(a*a*c+c*c*a+a*d*d+c*b*b)/((a+c)*(a+c)+(b+d)*(b+d));
p=(a*a*d+b*c*c+b*b*d+b*d*d)/((a+c)*(a+c)+(b+d)*(b+d));
   printf( "Result: %f+%fi \n",r, p);
return 0;
}
