#include <stdio.h>
#include <math.h>
int main(void) {
float a,b,c,x1,x2;
double D;
scanf("%f %f %f",&a,&b,&c);
D=pow(b,2)-4*a*c;
if (D>=0){
if (D>0){
x1=(-b+sqrt(D))/2*a;
x2=(-b-sqrt(D))/2*a;
printf("x1=%f,x2=%f\n",x1,x2);
}
else {
x1=-b/(2*a);
printf("x=%f\n",x1);
}
}
else printf("no solution\n");
return 0;
}
